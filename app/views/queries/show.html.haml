= link_to 'Back', queries_path

- @results.each do |result|
  .panel.panel-default
    .panel-heading
      = "#{result.contact_informations.first.name} - #{result.contact_informations.first.phone_numbers.first.number}"
    %table.table.table-bordered
      %tr
        %td{colspan: 12}= result.description
      %tr
        %th Address
        %th Price
        %th Bedrooms
        %th Bathrooms
        %th Garage
        %th Type
        %th Subtype
        %th Lot Size
        %th Square Feet
        %th Year Built
        %th School District
        %th Subdivision
      %tr
        %td= result.address
        %td= result.price
        %td= result.bedrooms
        %td= result.bathrooms
        %td= result.garage
        %td= result.result_type
        %td= result.subtype
        %td= result.lot_size
        %td= result.square_feet
        %td= result.year_built
        %td= result.school_district
        %td= result.subdivision
